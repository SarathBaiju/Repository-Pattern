@model IEnumerable<Student> ;
@{
    ViewData["Title"] = "Index";
}

<h1>Students</h1>

<div>
    <table class="table table-bordered">
        <tr>
            <td>Id</td>
            <td>Name</td>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>

            </tr>
        }
    </table>

</div>

